<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.starsea.im.biz.dao.UserEneryDao">


       <select id="queryEneryByOpenId"  resultType="String">
              select EnerySelect from test.UserEnery WHERE OpenId = #{openId}
       </select>


       <insert id="addUserEnery" parameterType="UserEneryEntity">
              insert into test.UserEnery(OpenId,EnerySelect)
              values(#{openId},#{enerySelect})
       </insert>

</mapper>